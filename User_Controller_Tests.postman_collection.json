		{
			"name": "TC001_Add_New_User",
			"item": [
				{
					"name": "Add_New_User",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Successful POST request, Validate status code from list\", function () {\r",
									"    pm.expect(pm.response.code).to.be.oneOf([200, 201, 202]);\r",
									"});\r",
									"\r",
									"pm.test(\"Response time should be less than 500ms\", function () {\r",
									"    pm.expect(pm.response.responseTime).to.be.below(500);\r",
									"});\r",
									"\r",
									"pm.test(\"Body contains: ID, cognito ID, name, address, Nino, Email and DOB\", function () {\r",
									"    pm.expect(pm.response.text()).to.include(\"id\", \"name\", \"address\", \"cognitoid\", \"nationalinsurancenumber\", \"email\", \"dateofbirth\");\r",
									"});\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// Generate a random email\r",
									"var randomEmail = 'user_' + Math.random().toString(36).substring(2, 12) + '@gmail.com';\r",
									"\r",
									"// Set the random email in the environment variable\r",
									"pm.environment.set(\"EMAIL\", randomEmail);"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"protocolProfileBehavior": {
						"strictSSL": false
					},
					"request": {
						"auth": {
							"type": "bearer",
							"bearer": {
								"token": "{{TOKEN}}"
							}
						},
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\r\n  \"cognitoid\": \"{{CognitoID}}\",\r\n  \"name\": \"{{Name}}\",\r\n  \"dateofbirth\": \"{{DOB}}\",\r\n  \"nationalinsurancenumber\": \"{{NINO}}\",\r\n  \"address\": \"{{ADDRESS}}\",\r\n  \"email\": \"{{EMAIL}}\"\r\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": "{{LSET_LINK}}/user/add"
					},
					"response": []
				}
			]
		},
